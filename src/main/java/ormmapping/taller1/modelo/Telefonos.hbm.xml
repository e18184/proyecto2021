<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.example.demo.modelo.Telefonos" table="telefonos" lazy="false">
		<composite-id>
			<key-many-to-one name="ORM_Codper" column="codper" class="com.example.demo.modelo.Personas">
			</key-many-to-one>
			<key-property name="numero" column="numero" type="string"/>
		</composite-id>
	</class>
</hibernate-mapping>
